{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "rtrox-containers/metadata",
  "title": "Chart Metadata",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "$schema": {
      "type": "string"
    },
    "artifactName": {
      "type": "string",
      "description": "Name of the Published Artifact",
      "pattern": "^[a-z0-9-]+$"
    },
    "name": {
      "type": "string",
      "description": "Name of the Upstream Chart",
      "pattern": "^[a-z0-9-]+$"
    },
    "version": {
      "type": "string",
      "description": "Version of the Upstream Chart"
    },
    "registry": {
      "type": "string",
      "description": "Registry of the Upstream Chart"
    }
  }
}
